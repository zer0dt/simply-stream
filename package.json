{
  "name": "simply-stream",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "turbo": "next dev --turbo",
    "start": "next start",
    "lint": "next lint",
    "preview": "next build && next start",
    "postinstall": "prisma generate",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "format": "npm run prettier . --write",
    "format:check": "npm run prettier . --check",
    "pretest": "npx scrypt-cli compile",
    "build:contract": "npx scrypt-cli compile",
    "deploy:contract": "npx ts-node ./scripts/deploy.ts",
    "verify:contract": "npx scrypt-cli verify $(cat .scriptHash) ./src/contracts/simplyStream.ts",
    "genprivkey": "npx ts-node ./scripts/privateKey.ts"
  },
